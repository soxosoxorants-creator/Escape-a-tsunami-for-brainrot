   Name = "",
   CurrentValue = false,
   Flag = "Toggle1", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   -- The function that takes place when the toggle is pressed
   -- The variable (Value) is a boolean on whether the toggle is true or false
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "Tsunami Danger Alert",
   CurrentValue = false,
   Flag = "Alert", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local player = game.Players.LocalPlayer
local pGui = player:WaitForChild("PlayerGui")

-- UI Creation
local sg = Instance.new("ScreenGui", pGui)
sg.Name = "BrainrotRadar"

local msg = Instance.new("TextLabel", sg)
msg.Size = UDim2.new(0, 350, 0, 60)
msg.Position = UDim2.new(0.5, -175, 0.1, 0)
msg.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
msg.TextColor3 = Color3.new(1, 1, 1)
msg.TextScaled = true
msg.Visible = false
msg.Font = Enum.Font.FredokaOne -- Fits the game's style

local function findWave()
    -- Wave of Brainrots usually uses "Tsunami" or "Water"
    return workspace:FindFirstChild("Tsunami") or workspace:FindFirstChild("Water")
end

task.spawn(function()
    while task.wait(0.1) do
        local wave = findWave()
        local char = player.Character
        if wave and char and char:FindFirstChild("HumanoidRootPart") then
            local dist = math.abs(char.HumanoidRootPart.Position.Z - wave.Position.Z)
            
            if dist < 1500 then
                msg.Visible = true
                msg.Text = "WAVE DISTANCE: " .. math.round(dist)
                
                -- Check for "Deadly" or "Fast" (usually Red or Purple)
                if wave.Color == Color3.fromRGB(255, 0, 0) then
                    msg.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
                    msg.Text = "⚠️ DEADLY WAVE: " .. math.round(dist)
                elseif wave.Color == Color3.fromRGB(170, 0, 255) then
                    msg.BackgroundColor3 = Color3.fromRGB(100, 0, 200)
                    msg.Text = "⚡ ELECTRIC WAVE: " .. math.round(dist)
                else
                    msg.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
                end
            else
                msg.Visible = false
            end
        else
            msg.Visible = false
        end
    end
end)
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "Wave Timer",
   CurrentValue = false,
   Flag = "Timer", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local player = game.Players.LocalPlayer
local pGui = player:WaitForChild("PlayerGui")

-- UI Creation
local sg = Instance.new("ScreenGui", pGui)
sg.Name = "WaveTimerUI"

local timerLabel = Instance.new("TextLabel", sg)
timerLabel.Size = UDim2.new(0, 250, 0, 50)
timerLabel.Position = UDim2.new(0.5, -125, 0.05, 0)
timerLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
timerLabel.BackgroundTransparency = 0.5
timerLabel.TextColor3 = Color3.new(1, 1, 0) -- Yellow for "Wait"
timerLabel.TextScaled = true
timerLabel.Font = Enum.Font.FredokaOne
timerLabel.Text = "Detecting Wave Cycle..."

-- Configuration for Wave of Brainrots
local INTERMISSION_TIME = 20 -- The average time between waves in this game
local lastWaveTime = tick()
local waveActive = false

local function findWave()
    return workspace:FindFirstChild("Tsunami") or workspace:FindFirstChild("Water")
end

task.spawn(function()
    while task.wait(0.1) do
        local wave = findWave()
        
        if wave then
            -- Wave is currently happening
            waveActive = true
            timerLabel.TextColor3 = Color3.new(1, 0, 0) -- Red for "Danger"
            timerLabel.Text = "WAVE ACTIVE"
            lastWaveTime = tick() -- Keep resetting the timer while wave exists
        else
            -- Wave is gone, we are in intermission
            if waveActive then
                waveActive = false
                lastWaveTime = tick() -- Start the countdown now
            end
            
            local timeSinceLastWave = tick() - lastWaveTime
            local timeLeft = math.max(0, INTERMISSION_TIME - timeSinceLastWave)
            
            timerLabel.TextColor3 = Color3.new(0, 1, 0) -- Green for "Safe to Run"
            
            if timeLeft > 0 then
                timerLabel.Text = string.format("Next Wave in: %.1fs", timeLeft)
            else
                timerLabel.Text = "WAVE IMMINENT! ⚠️"
                timerLabel.TextColor3 = Color3.new(1, 0.5, 0) -- Orange warning
            end
        end
    end
end)
   end,
})
local Tab = Window:CreateTab("Player", 4483362458) -- Title, Image
local Section = Tab:CreateSection("overpowerd")
local Toggle = Tab:CreateToggle({
   Name = "Extra Life
   CurrentValue = false,
   Flag = "Extra life", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")

-- Attempt to break the health cap
hum.MaxHealth = 9e9
hum.Health = 9e9

-- Connection to keep you alive
hum:GetPropertyChangedSignal("Health"):Connect(function()
    if hum.Health < hum.MaxHealth then
        hum.Health = hum.MaxHealth
    end
end)

print("Extra Life / God Mode Active. (Note: Only works if the game uses standard damage!)")
   end,
})
local Slider = Tab:CreateSlider({
   Name = "Jump hacks",
   Range = {0, 250},
   Increment = 10,
   Suffix = "Power",
   CurrentValue = 50, -- Set to 50 (Roblox default) so you don't start at 10
   Flag = "Slider1", 
   Callback = function(Value)
        -- Update the global variable so the loop below can see it
        _G.JumpPowerValue = Value
        
        -- Apply it immediately to the current character
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
            char.Humanoid.UseJumpPower = true
            char.Humanoid.JumpPower = Value
        end
   end,
})

-- THE FORCE LOOP (Put this at the very bottom of your script)
-- This prevents the game from resetting your jump back to default
task.spawn(function()
    while true do
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
            char.Humanoid.UseJumpPower = true
            -- If _G.JumpPowerValue exists, use it, otherwise use the slider's default
            char.Humanoid.JumpPower = _G.JumpPowerValue or 50
        end
        task.wait(0.1)
    end
end)
local Slider = Tab:CreateSlider({
   Name = "Speed hacks",
   Range = {0, 350}, -- Increased to 350 so you can outrun all wave types
   Increment = 1,   -- Changed to 1 for smoother control
   Suffix = "Speed",
   CurrentValue = 16, -- Roblox default speed
   Flag = "Slider2", 
   Callback = function(Value)
        -- Update the global variable for the loop
        _G.SpeedHackValue = Value
        
        -- Apply immediately
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
            char.Humanoid.WalkSpeed = Value
        end
   end,
})

-- THE FORCE LOOP (Place this at the bottom of your script)
task.spawn(function()
    while true do
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
            -- This forces the speed even if the game tries to reset it
            char.Humanoid.WalkSpeed = _G.SpeedHackValue or 16
        end
        task.wait(0.1)
    end
end)
local Toggle = Tab:CreateToggle({
   Name = "Infinite jump",
   CurrentValue = false,
   Flag = "infi jumps", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
  -- Variable to track the toggle state
local InfiniteJumpEnabled = false

-- Put this part inside your existing Toggle Callback
-- Callback = function(Value)
--    InfiniteJumpEnabled = Value
-- end

-- The actual logic that makes you jump in mid-air
game:GetService("UserInputService").JumpRequest:Connect(function()
    if InfiniteJumpEnabled then
        local character = game.Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                -- This forces the 'Jumping' state even if the character is in mid-air
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    end
end)
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "No clip",
   CurrentValue = false,
   Flag = "clip1", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local NoclipEnabled = false

-- Put this inside your Toggle Callback:
-- NoclipEnabled = Value

game:GetService("RunService").Stepped:Connect(function()
    if NoclipEnabled then
        local char = game.Players.LocalPlayer.Character
        if char then
            for _, part in pairs(char:GetChildren()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end
end)
   end,
})
local Tab = Window:CreateTab("Combat/HITBOXES", 4483362458) -- Title, Image
local Toggle = Tab:CreateToggle({
   Name = "Hitbox expander",
   CurrentValue = false,
   Flag = "box", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
  local HitboxEnabled = false
local HitboxSize = Vector3.new(20, 20, 20) -- Adjust size here

-- Put NoclipEnabled = Value inside your Toggle Callback

task.spawn(function()
    while true do
        if HitboxEnabled then
            -- In Wave of Brainrots, items are usually in workspace.Brainrots or workspace.Items
            -- This looks for everything you can pick up
            for _, v in pairs(workspace:GetDescendants()) do
                if v:IsA("TouchTransmitter") then
                    local part = v.Parent
                    if part and part:IsA("BasePart") then
                        part.Size = HitboxSize
                        part.CanCollide = false -- Prevents you from bumping into the giant invisible box
                        part.Transparency = 0.5 -- Optional: makes them look like big ghosts so you can see them
                    end
                end
            end
        end
        task.wait(1) -- Scans for new items every second
    end
end)
   end,
})
_G.HitboxSizeValue = 10 -- Initial value matching your CurrentValue

local Slider = Tab:CreateSlider({
   Name = "Hitbox size",
   Range = {0, 100},
   Increment = 10,
   Suffix = "Size",
   CurrentValue = 10,
   Flag = "Slider677", 
   Callback = function(Value)
      _G.HitboxSizeValue = Value
   end,
})

-- THE FORCE LOOP (Place this at the bottom of your script)
task.spawn(function()
    while true do
        -- Only run if the size is greater than 0
        if _G.HitboxSizeValue and _G.HitboxSizeValue > 0 then
            -- Loop through items in the game
            for _, v in pairs(workspace:GetDescendants()) do
                -- This targets anything that can be "touched" to collect
                if v:IsA("TouchTransmitter") then
                    local part = v.Parent
                    if part and part:IsA("BasePart") then
                        -- Resizes the item to your slider value
                        part.Size = Vector3.new(_G.HitboxSizeValue, _G.HitboxSizeValue, _G.HitboxSizeValue)
                        part.CanCollide = false -- So you don't trip over the massive items
                        part.Transparency = 0.7 -- Optional: makes the hitboxes easier to see
                    end
                end
            end
        end
        task.wait(2) -- Scans for new spawns every 2 seconds to prevent lag
    end
end)
local Tab = Window:CreateTab("Autos", 4483362458) -- Title, Image
local Toggle = Tab:CreateToggle({
   Name = "Auto Rebirth",
   CurrentValue = false,
   Flag = "rebirth1", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
  local AutoRebirthEnabled = false

-- Put AutoRebirthEnabled = Value inside your Toggle Callback

task.spawn(function()
    while true do
        if AutoRebirthEnabled then
            -- This is the most common path for Rebirth Remotes
            -- You may need to check Explorer for the exact name (e.g., "Rebirth", "AddRebirth")
            local remote = game:GetService("ReplicatedStorage"):FindFirstChild("Rebirth", true) 
            
            if remote and remote:IsA("RemoteEvent") then
                remote:FireServer()
            elseif remote and remote:IsA("RemoteFunction") then
                remote:InvokeServer()
            end
        end
        task.wait(1) -- Adjust this based on how fast you can rebirth
    end
end)
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "Auto collect cash",
   CurrentValue = false,
   Flag = "cash", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local TweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local AutoCashEnabled = false

-- Put AutoCashEnabled = Value inside your Toggle Callback

task.spawn(function()
    while task.wait(0.5) do
        if AutoCashEnabled and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            -- This looks for cash inside your base folder
            -- Replace "YourBaseName" with the actual name or use workspace.Drops
            for _, drop in pairs(workspace:GetDescendants()) do
                if drop.Name == "Cash" or drop.Name == "Coin" or drop:FindFirstChild("TouchTransmitter") then
                    -- Check if it's near your base (distance check)
                    local dist = (player.Character.HumanoidRootPart.Position - drop.Position).Magnitude
                    
                    if dist < 100 and AutoCashEnabled then -- Only grabs cash within 100 studs
                        local info = TweenInfo.new(0.3, Enum.EasingStyle.Linear)
                        local tween = TweenService:Create(player.Character.HumanoidRootPart, info, {CFrame = drop.CFrame})
                        
                        tween:Play()
                        tween.Completed:Wait() -- Wait until you actually touch the cash
                        task.wait(0.1) 
                    end
                end
                if not AutoCashEnabled then break end
            end
        end
    end
end)
   end,
})
