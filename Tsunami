local Tab = Window:CreateTab("Brainrots", 4483362458) -- Title, Image
local Section = Tab:CreateSection("Helpers")
local Toggle = Tab:CreateToggle({
   Name = "Instant grab",
   CurrentValue = false,
   Flag = "Grab", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

-- Most of these games store brainrots in a folder in Workspace
-- Common names are "Brainrots", "Collectibles", or "Drops"
local brainrotFolder = workspace:FindFirstChild("Brainrots") or workspace:FindFirstChild("Drops")

local function grabAll()
    if brainrotFolder then
        for _, item in pairs(brainrotFolder:GetChildren()) do
            -- Look for the pick-up prompt (usually E)
            local prompt = item:FindFirstChildWhichIsA("ProximityPrompt", true)
            if prompt then
                -- This "fires" the prompt instantly without you holding E
                fireproximityprompt(prompt)
            end
        end
    else
        warn("Could not find the Brainrot folder! Check the name in Explorer.")
    end
end

-- Run it once or set it to a loop
print("Grabbing all brainrots...")
grabAll()
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "ESP Highest worth Brainrot",
   CurrentValue = false,
   Flag = "esp", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local player = game.Players.LocalPlayer
local brainrotFolder = workspace:FindFirstChild("Brainrots") or workspace:FindFirstChild("Drops")

-- The "Wave of Brainrots" high-tier list
local RarityColors = {
    ["Celestial"] = Color3.fromRGB(255, 0, 255), -- Pink/Purple
    ["Infinity"] = Color3.fromRGB(0, 255, 255),  -- Cyan
    ["Secret"] = Color3.fromRGB(255, 0, 0),      -- Red
    ["Cosmic"] = Color3.fromRGB(0, 0, 255)       -- Blue
}

local function applyESP(item, color, name)
    if item:FindFirstChild("BrainrotESP") then return end

    -- Visual Highlight
    local hl = Instance.new("Highlight")
    hl.Name = "BrainrotESP"
    hl.FillColor = color
    hl.OutlineColor = Color3.new(1, 1, 1)
    hl.FillTransparency = 0.4
    hl.Parent = item

    -- Text Label
    local bg = Instance.new("BillboardGui")
    bg.Name = "BrainrotLabel"
    bg.AlwaysOnTop = true
    bg.Size = UDim2.new(0, 100, 0, 50)
    bg.Adornee = item
    bg.Parent = item

    local txt = Instance.new("TextLabel")
    txt.Size = UDim2.new(1, 0, 1, 0)
    txt.BackgroundTransparency = 1
    txt.TextColor3 = color
    txt.Text = "★ " .. name .. " ★"
    txt.Font = Enum.Font.SourceSansBold
    txt.TextSize = 16
    txt.TextStrokeTransparency = 0
    txt.Parent = bg
end

task.spawn(function()
    while true do
        if brainrotFolder then
            for _, item in pairs(brainrotFolder:GetChildren()) do
                for rarity, color in pairs(RarityColors) do
                    -- Checks if the rarity name is in the object's name
                    if string.find(item.Name, rarity) then
                        applyESP(item, color, item.Name)
                    end
                end
            end
        end
        task.wait(2)
    end
end)

print("ESP Loaded for Wave of Brainrots. Good luck hunting Celestials!")
   end,
})
