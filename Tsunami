local Tab = Window:CreateTab("Balant", 4483362458) -- Title, Image
local Section = Tab:CreateSection("Gaps")
local Toggle = Tab:CreateToggle({
   Name = "Gap 1",
   CurrentValue = false,
   Flag = "Gap 1", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local TweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")


local targetPosition = Vector3.new(0, 3, 150) -- Example X, Y, Z

local tweenInfo = TweenInfo.new(
    2, 
    Enum.EasingStyle.Quad, 
    Enum.EasingDirection.Out
)

local goal = {CFrame = CFrame.new(targetPosition)}
local tween = TweenService:Create(rootPart, tweenInfo, goal)

print("Yeeting to the safe zone...")
tween:Play()
   end,
})
local Toggle = Tab:CreateToggle({
   Name = "Gap 2",
   CurrentValue = false,
   Flag = "Gap 2", -- A flag is the identifier for the configuration file; make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local TweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:WaitForChild("HumanoidRootPart")

-- CHANGE THESE: Stand in the 2nd pit and use the print command to get these numbers
local targetPosition = Vector3.new(0, 3, 300) -- Example: 2nd pit is usually ~150 studs past the 1st

local tweenInfo = TweenInfo.new(
    2.5, -- Slightly longer time since the distance is further
    Enum.EasingStyle.Quad, 
    Enum.EasingDirection.Out
)

local goal = {CFrame = CFrame.new(targetPosition)}
local tween = TweenService:Create(rootPart, tweenInfo, goal)

-- Safety: Temporarily anchor so you don't fall or glitch during the slide
rootPart.Anchored = true
tween:Play()

tween.Completed:Connect(function()
    rootPart.Anchored = false
    print("Reached the 2nd Safe Zone. Rare area unlocked!")
end)
   end,
})
